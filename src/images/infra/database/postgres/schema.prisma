generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = 5
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_IMAGES_URL")
}

model Ubication {
  cod_ubi Int      @id
  country String   @db.VarChar(100)
  images  Images[]

  @@map("ubication")
}

model Descriptions {
  cod_description Int    @id
  description     String @db.VarChar(8)

  @@map("date_descriptions")
}

model Dates {
  cod_date Int      @id
  year     Int
  month    Int
  day      DateTime
  images   Images[]

  @@map("dates")
}

model Images {
  cod_image  String    @id @default(uuid()) @db.Uuid
  cod_ubi    Int
  cod_user   String    @db.Uuid
  image_path String
  uploadedat Int
  ubication  Ubication @relation(fields: [cod_ubi], references: [cod_ubi])
  uploaded   Dates     @relation(fields: [uploadedat], references: [cod_date])

  @@map("images")
}
